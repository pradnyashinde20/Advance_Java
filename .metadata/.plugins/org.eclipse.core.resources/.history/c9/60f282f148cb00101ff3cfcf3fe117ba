<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1" import="java.util.Set, com.demo.beans.Product" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
<%
Set<Product> cart = (Set<Product>)session.getAttribute("cart");
double totalamt =0;
%>
<table border=2>
<tr>
<th>Product Id</th>
<th>Product name</th>
<th>Product Quantity</th>
<th>price</th>
<th>amount</th>
</tr>
<%
for(Product p:cart){
	double amount = p.getQty()*p.getPrice();
	totalamt +=amount;
}
%>
</table>
</body>
</html>